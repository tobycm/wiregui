{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "wiregui",
  "version": "0.1.0",
  "identifier": "dev.tobycm.wiregui",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "wiregui",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/wiregui.png", "icons/wiregui@2x.png", "icons/wiregui@3x.png", "icons/wiregui.svg"]
  }
}
